C语言代码实现：
struct ListNode
{
  int m_nValue;
  ListNode* m_pNext;
}
void addToTail(ListNode** pHead,int value){
  ListNode* pNew=new ListNode();
  pNew->m_nValue=value;
  pNew->m_pNext=Null;
  if(*pHead==Null){
  *pHead=pNew;
  }else{
    ListNode*pNode=*pHead;
    while(pNode->m_pNext!=NULL){
      pNode=pNode->m_pNext;
      pNode->m_pNext=pNew;
    }
  }
}
